{
  "name": "number-unit",
  "description": "Numbers with units.",
  "version": "0.10.0",
  "author": "JP Richardson",
  "bugs": {
    "url": "https://github.com/exodusmovement/number-unit/issues"
  },
  "dependencies": {
    "bignumber.js": "^2.3.0",
    "clone": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "standard": "^7.1.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tape-catch": "^1.0.5"
  },
  "files": [
    "lib",
    "src"
  ],
  "homepage": "https://github.com/exodusmovement/number-unit#readme",
  "jsnext:main": "./src/index.js",
  "keywords": [
    "arbitrary",
    "big",
    "decimal",
    "number",
    "unit"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exodusmovement/number-unit.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint": "standard",
    "prepublish": "npm run test",
    "pretest": "npm run build",
    "test": "npm run lint && npm run unit",
    "unit": "find ./tests -name *.test.js -exec tape --require babel-register {} \\; | tap-spec"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib/"
    ]
  }
}
