{
  "name": "number-unit",
  "version": "0.6.0",
  "description": "Numbers with units.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel --stage 0 src --out-dir lib",
    "test": "standard && npm run build && find ./tests -name *.test.js -exec node --require ./tests/_babel.js {} \\; | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jprichardson/number-unit.git"
  },
  "keywords": [
    "number",
    "unit",
    "decimal",
    "big",
    "arbitrary"
  ],
  "author": "JP Richardson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jprichardson/number-unit/issues"
  },
  "homepage": "https://github.com/jprichardson/number-unit#readme",
  "devDependencies": {
    "babel": "5.x",
    "babel-eslint": "^4.1.5",
    "standard": "^5.4.1",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2"
  },
  "dependencies": {
    "bignumber.js": "^2.1.0",
    "clone": "^1.0.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib/"
    ]
  }
