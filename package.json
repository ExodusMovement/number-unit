{
  "name": "number-unit",
  "description": "Numbers with units.",
  "version": "0.10.0",
  "author": "JP Richardson",
  "bugs": {
    "url": "https://github.com/exodusmovement/number-unit/issues"
  },
  "dependencies": {
    "bignumber.js": "^2.1.0",
    "clone": "^1.0.2"
  },
  "devDependencies": {
    "babel": "5.x",
    "babel-eslint": "^4.1.5",
    "standard": "^5.4.1",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2"
  },
  "files": [
    "lib",
    "src"
  ],
  "homepage": "https://github.com/exodusmovement/number-unit#readme",
  "jsnext:main": "./src/index.js",
  "keywords": [
    "arbitrary",
    "big",
    "decimal",
    "number",
    "unit"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exodusmovement/number-unit.git"
  },
  "scripts": {
    "build": "babel --stage 0 src --out-dir lib",
    "lint": "standard",
    "test": "npm run lint && npm run unit",
    "unit": "npm run build && find ./tests -name *.test.js -exec node --require ./tests/_babel.js {} \\; | tap-spec"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib/"
    ]
  }
}
